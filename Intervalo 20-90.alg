algoritmo "Intervalo entre 20 e 90"

var
n: inteiro
a: real
r: inteiro
inicio

escreva ("Digite um número: ")
leia (n)
escreval ()

se (n>=20) e (n<=90) entao
escreval ("O número",n," está DENTRO do intervalo [20,90]")
senao
escreval ("O número",n," está FORA do intervalo [20,90]")
fimse


fimalgoritmo
